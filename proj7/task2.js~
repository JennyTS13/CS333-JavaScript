var num = 1; //memory is allocated for a number
var string = "Tiffany & Jenny"; //memory is allocated for a string
var Person = function ( first, last, mi, age ){
    this.firstName = first;
    this.lastName = last;
    this.MI = mi;
    this.age = age;
}; //memory is allocated for an object
